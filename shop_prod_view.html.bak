				<script type="text/javascript">
				//header height
				var header = $('header');
				var headerH = $('header').outerHeight();
				var bannerH = $('.banner').outerHeight();
				var contH;
				var docH;
				var wrap = $(".wrap");
				var gnbAllMenuBtn = $(".gnb-all-menu");
				var bannerCloseBtn = $(".banner .close");

				function contentH() {
					docH = $(window).innerHeight();
					contH = docH - (headerH + bannerH);
					//mobile content height
					mobileGnbUnitHeight();
				}

				function mobileGnbUnitHeight() {
					if ($(window).innerWidth() < 720) {
						$(".gnb-all-wrap").css("height",contH + "px");
						$(".search-wrap").css("height",contH + "px");
					}
				}

				//mobile
				function bannerXContentH() {
					docH = $(window).innerHeight();
					contH = docH - headerH;
					$(".gnb-all-wrap").css("height",contH + "px");
					$(".search-wrap").css("height",contH + "px");
					visualHeightStyle();
				}

				function gnbNull() {
					$(".gnb-wrap").css("pointerEvents","none");
				}

				function gnbActive() {
					$(".gnb-wrap").css("pointerEvents","auto");
				}

				function gnbAllBtnNull() {
					$(".gnb-all-menu").css("pointerEvents","none");
				}

				function gnbAllBtnActive() {
					$(".gnb-all-menu").css("pointerEvents","auto");
				}


				//pc visual height
				function visualHeightStyle() {
					$(".visual").css("height",contH + "px");
				}

				function resizeActive() {
					contentH();
						visualHeightStyle();
				}

				function searchInfo(){
					if($("#search_info").val()==""){
							alert("검색어를 입력 해주세요.");
							return false;
					}
					
					location.href = "search_list.html");
				}

				$(document).ready(function() {
					// 검색어 Enter 이벤트
					$("#search_info").keydown(function(key) {
						if (key.keyCode == 13) {        			
							searchInfo();
						}
					});
					
					//검색창
					$(".search-open").on("click",function(){
						$(".search-wrap").addClass("open");
						gnbNull();
						gnbAllBtnNull();
					});

					$(".search-close").on("click",function(){
						$(".search-wrap").removeClass("open");
						gnbActive();
						gnbAllBtnActive();
					});

					//dim click search, gnb all menu close
					$(".dim").on("click",function(){
						$("#menuToggle").find("input[type='checkbox']").prop("checked", false);
						$(".search-wrap").removeClass("open");
						$(".wrap").removeClass("gnb-open");
						gnbActive();
						gnbAllBtnActive();
					});

					$(".banner .close").on("click",function(){
						$(".wrap").addClass("ban-x");
						resizeActive();
						if ($(window).innerWidth() < 720) {
							bannerXContentH();
						}
					});

					$(".gnb-all-menu").on("click",function(){
						if($(".wrap").hasClass("gnb-open")){
							$(".wrap").removeClass("gnb-open");
							gnbActive();
						}else{
							$(".wrap").addClass("gnb-open");
							gnbNull();
						}
					});
					
					contentH();
						visualHeightStyle();
				});

				$(window).resize(function(){
					resizeActive();
				});
				</script>
